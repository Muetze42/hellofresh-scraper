{
    "name": "norman-huth/hellofresh-scraper",
    "description": "Web Scraper for HelloFresh data.",
    "keywords": [
        "hellofresh",
        "laravel-package",
        "recipes",
        "scraper"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-dom": "*",
        "ext-libxml": "*",
        "illuminate/http": "^10.43|^11.0",
        "illuminate/support": "^10.43|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^9.4",
        "larastan/larastan": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "NormanHuth\\HellofreshScraper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Norman Huth",
            "homepage": "https://huth.it"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "NormanHuth\\HellofreshScraper\\PackageServiceProvider"
            ]
        }
    }
}
